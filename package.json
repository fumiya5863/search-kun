{
  "name": "search-kun",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "concurrently \"npm run server\" \"cross-env NODE_ENV=develop npm run electron\"",
    "server": "webpack-dev-server",
    "electron": "wait-on http://localhost:8080 && electron \"./app\"",
    "webpack": "webpack",
    "build": "npm run format && npm run webpack-build",
    "webpack-build": "webpack --mode=production",
    "format": "prettier --config \"./.prettierrc.json\" --write \"./src\"",
    "format-watch": "onchange \"./src\" -- prettier --config ./.prettierc.js --write \"./src\""
  },
  "keywords": [],
  "author": "fumiya5863",
  "license": "MIT",
  "dependencies": {
    "bootstrap": "^4.6.0",
    "electron": "^12.0.1",
    "electron-packager": "^15.0.0",
    "react": "^17.0.1",
    "react-bootstrap": "^1.5.0",
    "react-dom": "^17.0.1",
    "react-icons": "^4.2.0",
    "react-router-dom": "^5.2.0",
    "stisla-theme": "^1.0.6",
    "styled-components": "^5.2.0",
    "styled-icons": "^10.29.0"
  },
  "devDependencies": {
    "@babel/core": "^7.12.17",
    "@babel/preset-env": "^7.12.17",
    "@babel/preset-react": "^7.12.13",
    "babel-loader": "^8.2.2",
    "babel-plugin-react-html-attrs": "^3.0.5",
    "concurrently": "^6.0.0",
    "cross-env": "^7.0.3",
    "css-loader": "^5.1.1",
    "husky": "^5.1.3",
    "lint-staged": "^10.5.4",
    "onchange": "^7.1.0",
    "prettier": "^2.2.1",
    "style-loader": "^2.0.0",
    "url-loader": "^4.1.1",
    "wait-on": "^5.2.1",
    "webpack": "^4.19.1",
    "webpack-cli": "^3.1.0",
    "webpack-dev-server": "^3.1.8"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{css,js}": [
      "prettier --write \"./src\""
    ]
  }
}
